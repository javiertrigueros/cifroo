{% extends '::backend.html.twig' %}

{% block container %}
    
<div class="modal-load modal-load-fix">
    <div class="ar-container">
           <div class="item-swip cssload-piano">
            <div class="cssload-rect1"></div>
            <div class="cssload-rect2"></div>
            <div class="cssload-rect3"></div>
          </div>
     </div>
</div>
    
<div id="content-wrapper" class="px-content">
          
</div>
    
<div class="hide">
{{ form(form, {'attr': {'id': 'form-arFile'}}) }}
</div>
     
{% endblock %}

{% block stylesheets %}
    {{ parent() }}
    <link href="{{ asset('bundles/wall/css/animation_wait.css') }}" rel="stylesheet">
    <link href="{{ asset('bundles/wall/css/explorer.css') }}" rel="stylesheet">
    <link href="{{ asset('bundles/wall/js/vendor/explorer/app/styles/main.css') }}" rel="stylesheet">
    
    <style>
        .article-content
        {
          overflow: auto;  
        }
        .selected-icon
        {
            color:white !important;
        }
        .icon-select-extra
        {
            padding-left: 6px;
            padding-right: 3px;
            padding-top: 5px;
            padding-bottom: 5px;
        }
        .icon-select
        {
            padding-left: 9px;
            padding-right: 6px;
            padding-top: 5px;
            padding-bottom: 5px;
        }
        
        .label-select-text
        {
            margin-left: 10px;
        }
        header h1
        {
            margin-top: 0px;
            margin-bottom: 15px;
        }
        
    </style>
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    <script src="{{ asset('bundles/wall/js/bower_components/blob-util/dist/blob-util.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/wall/js/vendor/explorer/app/bower_components/modernizr/modernizr.js') }}" type="text/javascript"></script>
    <script data-main="/bundles/wall/js/vendor/explorer/app/scripts/main.js" src="{{ asset('bundles/wall/js/vendor/explorer/app/bower_components/requirejs/require.js') }}"></script>
{% endblock %}
